{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "features": {
    "ghcr.io/devcontainers-contrib/features/tmux-apt-get:1": {},
    "ghcr.io/guiyomh/features/vim:0": {}
    },
  "waitFor": "onCreateCommand",
  "updateContentCommand": [
    "conda install pytorch torchvision torchaudio pytorch-lightning -c pytorch -c conda-forge"
    ],
  "customizations": {
    "codespaces": {
      "openFiles": []
    },
    "vscode": {
      "extensions": [
        "ms-toolsai.jupyter",
        "ms-python.python"
      ]
  }
}
